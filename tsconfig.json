{
    "compilerOptions": {
        "sourceMap": true,
        "noImplicitAny": false,
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "jsx": "preserve",
        "esModuleInterop": true,
        "allowJs": true,
        "experimentalDecorators": true,
        "useDefineForClassFields": false,
    },
    // "lib": ["dom", "esnext", "node"],
    "allowSyntheticDefaultImports": true,
    "include": [
        "./src/**/*"
    ]
}